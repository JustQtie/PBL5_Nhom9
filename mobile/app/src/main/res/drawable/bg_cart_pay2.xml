<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Hình dạng với màu nền và viền -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#F4DCA0" /> <!-- Màu nền xanh nhạt -->
            <corners android:radius="18dp" /> <!-- Bo góc -->
            <stroke android:width="2dp" android:color="@color/green" /> <!-- Viền màu xanh lá -->
        </shape>
    </item>

</layer-list>
